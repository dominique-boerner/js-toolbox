    ],
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "module": "index.js",
    "name": "@xcmats/js-toolbox",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drmats/js-toolbox.git"
    },
    "scripts": {
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel ./src/ -d ./lib/",
        "build:es": "cross-env BABEL_ENV=es babel ./src/ -d ./",
        "build": "npm run clean:build && npm run build:commonjs",
        "clean:build": "rimraf ./lib",
        "clean:doc": "rimraf ./doc",
        "clean:modules": "rimraf package-lock.json && rimraf node_modules",
        "clean": "npm run clean:build && npm run clean:doc",
        "deploy": "gh-pages -d doc --message 'gh-pages update'",
        "doc": "npm run clean:doc && npm run jsdoc",
        "jsdoc": "echo \"Generating jsdoc ...\" && jsdoc -c ./jsdoc.json -R ./README.md ./src/ -d ./doc/",
        "lint": "eslint ./src/",
        "postpack": "node ./scripts/delete.js",
        "predeploy": "npm run doc",
        "prepack": "npm run build:es && node ./scripts/copy.js",
        "prepublishOnly": "npm run test && npm run doc && npm run build",
        "prestart": "npm run build:commonjs",
        "start": "node ./scripts/start.js",
        "test": "npm run lint && echo \"Warning: no tests specified (yet)\""
    },
    "typings": "index.d.ts",
    "version": "0.9.5"
}
