        }
    ],
    "dependencies": {
        "@babel/runtime": "^7.0.0-beta.49"
    },
    "description": "Various JS utilities.",
    "devDependencies": {
        "@babel/cli": "^7.0.0-beta.49",
        "@babel/core": "^7.0.0-beta.49",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
        "@babel/plugin-transform-arrow-functions": "^7.0.0-beta.49",
        "@babel/plugin-transform-block-scoped-functions": "^7.0.0-beta.49",
        "@babel/plugin-transform-block-scoping": "^7.0.0-beta.49",
        "@babel/plugin-transform-computed-properties": "^7.0.0-beta.49",
        "@babel/plugin-transform-destructuring": "^7.0.0-beta.49",
        "@babel/plugin-transform-exponentiation-operator": "^7.0.0-beta.49",
        "@babel/plugin-transform-literals": "^7.0.0-beta.49",
        "@babel/plugin-transform-parameters": "^7.0.0-beta.49",
        "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
        "@babel/plugin-transform-shorthand-properties": "^7.0.0-beta.49",
        "@babel/plugin-transform-spread": "^7.0.0-beta.49",
        "@babel/plugin-transform-template-literals": "^7.0.0-beta.49",
        "@babel/preset-env": "^7.0.0-beta.49",
        "babel-eslint": "^8.2.3",
        "cross-env": "^5.1.6",
        "eslint": "^4.19.1",
        "jsdoc": "^3.5.5",
        "rimraf": "^2.6.2"
    },
    "engines": {
        "node": ">=6.0.0",
        "npm": ">=5.0.0"
    },
    "files": [
        "doc",
        "es",
        "index.d.ts",
        "lib",
        "src"
    ],
    "homepage": "https://github.com/drmats/js-toolbox",
    "keywords": [
        "es6",
        "javascript",
        "library",
        "tools",
        "snippets"
    ],
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "module": "es/index.js",
    "name": "@xcmats/js-toolbox",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drmats/js-toolbox.git"
    },
    "scripts": {
        "build": "npm run clean:build && npm run build:commonjs && npm run build:es",
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel ./src/ -d ./lib/",
        "build:es": "cross-env BABEL_ENV=es babel ./src/ -d ./es/",
        "clean": "npm run clean:build && npm run clean:doc",
        "clean:build": "rimraf ./es ./lib",
        "clean:doc": "rimraf ./doc",
        "clean:modules": "rimraf node_modules",
        "doc": "npm run clean:doc && npm run jsdoc",
        "jsdoc": "jsdoc -c ./jsdoc.json -R ./README.md ./src/ -d ./doc/",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run test && npm run doc && npm run build",
        "start": "npm run build:commonjs && node ./scripts/start.js",
        "test": "npm run lint && echo \"Warning: no tests specified (yet)\""
    },
    "types": "./index.d.ts",
    "version": "0.3.4"
}
