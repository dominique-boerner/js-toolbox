{
    "bugs": {
        "url": "https://github.com/drmats/js-toolbox/issues"
    },
    "contributors": [
        {
            "name": "drmats",
            "email": "xcmats@protonmail.com",
            "url": "https://twitter.com/xcmats/"
        }
    ],
    "dependencies": {
        "@babel/runtime": "^7.9.2"
    },
    "description": "Useful JS utilities.",
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-arrow-functions": "^7.8.3",
        "@babel/plugin-transform-block-scoped-functions": "^7.8.3",
        "@babel/plugin-transform-block-scoping": "^7.8.3",
        "@babel/plugin-transform-computed-properties": "^7.8.3",
        "@babel/plugin-transform-destructuring": "^7.9.5",
        "@babel/plugin-transform-exponentiation-operator": "^7.8.3",
        "@babel/plugin-transform-literals": "^7.8.3",
        "@babel/plugin-transform-modules-commonjs": "^7.9.0",
        "@babel/plugin-transform-parameters": "^7.9.5",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/plugin-transform-shorthand-properties": "^7.8.3",
        "@babel/plugin-transform-spread": "^7.8.3",
        "@babel/plugin-transform-template-literals": "^7.8.3",
        "@babel/preset-env": "^7.9.5",
        "babel-eslint": "^10.1.0",
        "babel-plugin-inline-json-import": "^0.3.2",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "gh-pages": "^2.2.0",
        "jsdoc": "^3.6.4",
        "minami": "^1.2.3",
        "rimraf": "^3.0.2"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=6.0.0"
    },
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/drmats/js-toolbox",
    "keywords": [
        "array",
        "async",
        "byte-array",
        "codec",
        "es6",
        "functional",
        "javascript",
        "library",
        "string",
        "type"
    ],
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "module": "dist/es/index.js",
    "name": "@xcmats/js-toolbox",
    "private": true,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drmats/js-toolbox.git"
    },
    "scripts": {
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel ./src/ -d ./dist/",
        "build:es": "cross-env BABEL_ENV=es babel ./src/ -d ./dist/es/",
        "build": "npm run clean:build && npm run build:prepare && npm run build:commonjs && npm run build:es",
        "build:prepare": "node ./scripts/prepare.js",
        "clean:build": "rimraf ./dist",
        "clean:doc": "rimraf ./dist/doc",
        "clean:modules": "rimraf package-lock.json && rimraf node_modules",
        "clean": "npm run clean:build && npm run clean:doc",
        "deploy": "gh-pages -d ./dist/doc --message 'gh-pages update'",
        "doc": "npm run clean:doc && npm run jsdoc",
        "jsdoc": "echo \"Generating jsdoc ...\" && jsdoc -c ./jsdoc.json -R ./README.md ./src/**/* -d ./dist/doc/",
        "lint": "eslint ./src/",
        "prepublishOnly": "npm run test && npm run build && npm run doc",
        "prestart": "node ./scripts/prepare.js && npm run build:commonjs",
        "publishAll": "npm run prepublishOnly && echo \"Publishing ...\" && cd ./dist/ && npm publish && cd .. && npm run deploy && echo \"Done.\"",
        "start": "node ./scripts/start.js",
        "test": "npm run lint && echo \"Warning: no tests specified (yet)\""
    },
    "typings": "dist/index.d.ts",
    "version": "0.9.7"
}
